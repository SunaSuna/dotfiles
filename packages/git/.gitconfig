[user]
	name = sunagawa_masahide
	email = suna@sfidante.co.jp
[core]
	editor = vim -c \"set fenc=utf-8\"
	precomposeunicode = true
	quotepath = false
	whitespace = cr-at-eol
	excludesfile = /Users/masahide/.gitignore_global
[color]
	diff = auto
	status = auto
	branch = auto
[alias]
	tr = log --graph --all --date=short --format=\"%C(yellow)%h%C(reset) %C(magenta)[%ad]%C(reset)%C(auto)%d%C(reset) %s %C(cyan)@%an%C(reset)\"
